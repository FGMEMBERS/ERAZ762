 </emission>
</animation>

<animation>
<type>pick</type>
<object-name>door</object-name>
<visible>true</visible>
<action>
<button>0</button>
<repeatable>false</repeatable>
<binding>
<command>nasal</command>
<script>aucafly.door.toggle();</script>
</binding>
</action>
<hovered>
